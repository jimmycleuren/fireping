{
    "name": "jimmycleuren/fireping",
    "description": "A tool to monitor network latency",
    "license": "GPL-3.0-or-later",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\DataFixtures\\": "fixtures/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-rrd": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.5",
        "easycorp/easyadmin-bundle": "^3.0",
        "eightpoints/guzzle-bundle": "^8.1",
        "flexihash/flexihash": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "kevinpapst/adminlte-bundle": "^3.2",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.1",
        "react/event-loop": "^1.0",
        "react/socket": "^1.0",
        "react/stream": "^1.0",
        "sensio/framework-extra-bundle": "^6.1",
        "snc/redis-bundle": "^3.2",
        "symfony/asset": "^5.0",
        "symfony/cache": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^5.0",
        "symfony/lock": "^5.0",
        "symfony/monolog-bundle": "^3.2",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/process": "^5.0",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/proxy-manager-bridge": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "twig/twig": "^3.0.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "mi-schi/phpmd-extension": "^4.2",
        "phpmd/phpmd": "^2.6",
        "phpspec/prophecy": "^1.19",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^5.0",
        "symfony/web-profiler-bundle": "^5.0"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "vendor/bin/phpstan --level=5 --configuration=phpstan.neon analyse src",
        "test": {
            "tests": "bin/phpunit --coverage-html public/coverage --debug"
        },
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 0,
        "allow-plugins": {
            "symfony/flex": true,
            "phpstan/extension-installer": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^5.0"
        }
    }
}

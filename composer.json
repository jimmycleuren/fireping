{
    "name": "jimmycleuren/fireping",
    "license": "GPL-3.0",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-rrd": "*",
        "api-platform/api-pack": "^1.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.5",
        "easycorp/easyadmin-bundle": "^2.0",
        "eightpoints/guzzle-bundle": "^8.1",
        "flexihash/flexihash": "^2.0",
        "friendsofsymfony/user-bundle": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "predis/predis": "^1.1",
        "react/event-loop": "^1.0",
        "react/socket": "^1.0",
        "react/stream": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",
        "snc/redis-bundle": "^3.2",
        "symfony/asset": "^4.0",
        "symfony/cache": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/lock": "^4.1",
        "symfony/monolog-bundle": "^3.2",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/process": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "mi-schi/phpmd-extension": "^4.2",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "vendor/bin/phpstan --level=4 --configuration=phpstan.neon analyse src",
        "test": {
            "tests": "vendor/bin/phpunit --coverage-html ../fireping-coverage"
        },
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 0
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "branch-alias": null
    }
}
